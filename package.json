{
  "name": "alexa-presentation-language-preview",
  "displayName": "Alexa Presentation Language Preview",
  "description": "Unofficial preview tool for Alexa Presentation Language",
  "version": "0.1.0",
  "engines": {
    "vscode": "^1.70.0"
  },
  "publisher": "YoshiakiYamada",
  "keywords": [
    "Alexa",
    "JSON",
    "APL",
    "Local Debug",
    "Alexa skills",
    "Alexa Presentation Language"
  ],
  "categories": [
    "Visualization",
    "Testing"
  ],
  "activationEvents": [
    "onCommand:alexa-presentation-language-preview.previewApl"
  ],
  "main": "./out/extension.js",
  "contributes": {
    "commands": [
      {
        "command": "alexa-presentation-language-preview.previewApl",
        "title": "Preview Alexa Presentation Language"
      }
    ]
  },
  "icon": "icon.png",
  "license": "SEE LICENSE IN LICENSE",
  "scripts": {
    "vscode:prepublish": "yarn run compile",
    "compile": "tsc -p ./",
    "watch": "tsc -watch -p ./",
    "pretest": "yarn run compile && yarn run lint",
    "lint": "eslint src --ext ts",
    "test": "node ./out/test/runTest.js"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/ysak-y/alexa-presentation-language-preview.git"
  },
  "devDependencies": {
    "@types/glob": "^7.2.0",
    "@types/mocha": "^9.1.1",
    "@types/node": "16.x",
    "@types/vscode": "^1.70.0",
    "@typescript-eslint/eslint-plugin": "^5.31.0",
    "@typescript-eslint/parser": "^5.31.0",
    "@vscode/test-electron": "^2.1.5",
    "eslint": "^8.20.0",
    "glob": "^8.0.3",
    "mocha": "^10.0.0",
    "typescript": "^4.7.4"
  },
  "dependencies": {
    "apl-suggester": "^2022.1.0",
    "apl-viewhost-web": "^2022.1.0",
    "merge": "^2.1.1"
  }
}
